@model List<ResultAppUserListDto>
@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}



                        <table class="table table-xs mb-0">
                            <thead>
                                <tr>
                                    <th>Kullanıcı</th>
                                    <th>Şehir</th>
                                    <th>Ülke</th>
                                    <th>Durum</th>
                                    <th>Cinsiyet</th>
                                
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td><img src="@item.ImageUrl" class=" rounded-circle mr-3" alt="">@item.Name</td>
                                    <td>@item.City</td>
                                    <td>
                                        <span>Türkiye</span>
                                    </td>
                                    <td>
                                        <div>
                                            <div class="progress" style="height: 6px">
                                                <div class="progress-bar bg-success" style="width: 50%"></div>
                                            </div>
                                        </div>
                                    </td>
                                    <td><i class="fa fa-circle-o text-success  mr-2"></i>Erkek</td>
                                    
                                </tr>
                                }
                            </tbody>
                        </table>


<br />
<a href="/Register/Index/" class="btn btn-primary">Yeni Kullanıcı Kaydı</a>
